  "author": "Netzer Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nforceai/rxzu"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.21.1"
  },
  "private": false,
  "scripts": {
    "prepare": "husky install",
    "ng": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint:affected": "nx affected:lint --fix",
    "angular:storybook": "yarn docs:json && nx run angular:storybook",
    "docs:json": "compodoc -p ./libs/angular/tsconfig.lib.prod.json -e json -d ./libs/angular",
    "run-many": "yarn docs:json && npx nx run-many --targets=build,build-storybook --configuration=production --all",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:lint:fix": "nx affected:lint --parallel --uncommitted --only-failed --fix",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "dep-graph": "nx dep-graph"
  },
  "dependencies": {
    "@angular/common": "15.2.0",
    "@angular/core": "15.2.0",
    "@angular/platform-browser": "15.2.0",
